{
  "expo": {
    "name": "bookingApp",
    "slug": "bookingApp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "react-native-fbsdk-next",
        {
          "appID": "1403305993896132",
          "clientToken": "930bf9c82e1552164349751b77872d90",
          "displayName": "Booking App",
          "scheme": "fb1403305993896132"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
